---
import "@fontsource-variable/assistant";
import "@fontsource-variable/cinzel";
import { SEO } from "astro-seo";
import "../assets/style.css";
import "aos/dist/aos.css";
interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="keywords"
      content="Logic Moon, Tobias Lorsbach, Ambient music, Drone, Neoclassical, texture, music"
    />
    <meta
      name="description"
      content="Logic Moon is the ambient music project of Tobias Lorsbach. His music is a blend of ambient, drone, and neoclassical music."
    />
    <meta name="author" content="Tobias Lorsbach" />
    <meta name="viewport" content="width=device-width" />
    <link
      href="data:image/x-icon;base64,"
      rel="icon"
      type="image/x-icon"
    />
    <title>{title}</title>
    <script>
      import AOS from "aos";
      import Lenis from "@studio-freight/lenis";
      const lenis = new Lenis();

      // lenis.on('scroll', (e) => {
      //   console.log(e)
      // })

      function raf(time) {
        lenis.raf(time);
        requestAnimationFrame(raf);
      }

      requestAnimationFrame(raf);
      document.onreadystatechange = function () {
        AOS.init();
      };
    </script>

    <script>
      var _paq = (window._paq = window._paq || []);
      _paq.push(["trackPageView"]);
      _paq.push(["enableLinkTracking"]);
      (function () {
        var u = "//matomo.tobeworks.de/";
        _paq.push(["setTrackerUrl", u + "matomo.php"]);
        _paq.push(["setSiteId", "2"]);
        var d = document,
          g = d.createElement("script"),
          s = d.getElementsByTagName("script")[0];
        g.async = true;
        g.src = u + "matomo.js";
        s.parentNode.insertBefore(g, s);
      })();
    </script>
  </head>
  <body>
    <slot />
  </body>
</html>
